    "vue": "^2.6.11",
    "vue-awesome": "^4.0.2",
    "vue-good-table": "^2.19.2",
    "vue-multiselect": "^2.1.6",
    "vue-notification": "^1.3.20",
    "vue-router": "^3.1.6",
    "vuedraggable": "^2.23.2",
    "vuejs-datepicker": "^1.6.2",
    "vuejs-logger": "^1.5.4",
